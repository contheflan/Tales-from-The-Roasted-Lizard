h":"77","messages":"78","errorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0,"usedDeprecatedRules":"42"},"/Users/connormckenna/General_Assembly/sei/phoenix/unit_4/Tales-from-The-Roasted-Lizard/client/src/index.js",[],"/Users/connormckenna/General_Assembly/sei/phoenix/unit_4/Tales-from-The-Roasted-Lizard/client/src/reportWebVitals.js",[],["79","80"],"/Users/connormckenna/General_Assembly/sei/phoenix/unit_4/Tales-from-The-Roasted-Lizard/client/src/App.js",["81","82","83","84"],"import { useState, useEffect } from 'react';\nimport { Route, useHistory } from 'react-router-dom';\nimport \"./App.css\";\nimport Intro from \"./screens/Intro/Intro\";\nimport Login from \"./screens/Login/Login\";\nimport Register from \"./screens/Register/Register\";\nimport AccountDetails from \"./screens/AccountDetails/AccountDetails\";\nimport Postings from \"./screens/Postings/Postings\";\nimport CreatePosting from \"./screens/CreatePosting/CreatePosting\";\nimport PostingDetails from \"./screens/PostingDetails/PostingDetails\";\nimport EditPosting from \"./screens/EditPosting/EditPosting\";\nimport CreateComment from \"./screens/CreateComment/CreateComment\";\nimport { destroyPosting, getAllPostings, postPosting, putPosting } from \"../src/services/postings\"\nimport { postComment } from \"../src/services/comments\"\nimport { loginUser, registerUser, removeToken, verifyUser } from './services/auth';\n\nfunction App() {\n  const [toggle, setToggle] = useState(true)\n  const [currentUser, setCurrentUser] = useState(null);\n  const [postings, setPostings] = useState([]);\n  const [comments, setComments] = useState([]);\n  const history = useHistory()\n\n  useEffect(() => {\n    const handleVerify = async () => {\n      const userData = await verifyUser();\n      setCurrentUser(userData);\n      if (!userData) {\n        history.push('/')\n      }\n    }\n    handleVerify();\n\n    const fetchPostings = async () => {\n      const postingData = await getAllPostings();\n      setPostings(postingData);\n    }\n    fetchPostings();\n\n  }, [toggle])\n\n  const handleLogin = async (loginData) => {\n    const userData = await loginUser(loginData);\n    setCurrentUser(userData);\n    history.push('/Postings');\n  }\n\n  const handleRegister = async (registerData) => {\n    const userData = await registerUser(registerData);\n    setCurrentUser(userData);\n    history.push('/Login');\n  }\n\n  const handleLogout = () => {\n    setCurrentUser(null);\n    localStorage.removeItem('authToken');\n    removeToken();\n    history.push('/');\n  }\n\n  const handlePostCreate = async (postingData) => {\n    const newPosting = await postPosting(postingData);\n    setPostings(prevState => [...prevState, newPosting]);\n    history.push('/Postings');\n  }\n\n  const handlePostDelete = async (id) => {\n    await destroyPosting(id);\n    setPostings(prevState => prevState.filter(posting => posting.id !== id));\n    setToggle(prevState => !prevState)\n    history.push('/Postings');\n  }\n\n  const handlePostUpdate = async (postingData) => {\n    const newPosting = await postPosting(postingData);\n    setPostings(prevState => [...prevState, newPosting]);\n    history.push(`/Postings/${newPosting.id}`);\n  }\n\n  const handleCommentCreate = async (commentData, posting_id) => {\n    const newComment = await postComment(commentData, posting_id);\n    setComments(prevState => [...prevState, newComment]);\n    setToggle(prevState => !prevState)\n    history.push(`/Postings/${posting_id}`);\n    // is this line above right? ask shay\n  }\n\n  return (\n    <div className=\"App\">\n      <div>\n      <Route exact path=\"/\">\n        <Intro />\n      </Route>\n        \n      <Route exact path=\"/Login\">\n        <Login \n          handleLogin={handleLogin} />\n      </Route>\n        \n      <Route exact path=\"/Register\">\n        <Register \n          handleRegister={handleRegister} />\n      </Route>\n        \n      <Route exact path=\"/AccountDetails\">\n        <AccountDetails \n          handleLogout={handleLogout} />\n      </Route>\n        \n      <Route exact path=\"/Postings\">\n        <Postings\n          postings={postings}\n          handleLogout={handleLogout}\n          handlePostDelete={handlePostDelete} />\n      </Route>\n        \n      <Route exact path=\"/Postings/:id\">\n          <PostingDetails\n          postings={postings}\n          handleLogout={handleLogout}\n          handlePostDelete={handlePostDelete} />\n      </Route>\n        \n      <Route exact path=\"/CreatePosting\">\n          <CreatePosting\n          postings={postings}\n          handleLogout={handleLogout}\n          handlePostCreate={handlePostCreate} />\n      </Route>\n        \n      <Route exact path=\"/Postings/:id/Edit\">\n        <EditPosting\n          postings={postings}\n          handlePostUpdate={handlePostUpdate}\n          handlePostDelete={handlePostDelete} />\n      </Route>\n\n      <Route exact path=\"/Postings/:id/comments\">\n        <CreateComment\n          handleCommentCreate={handleCommentCreate} />\n      </Route>\n      \n      <Route exact path=\"/EditComment\">\n        \n      </Route>\n      </div>\n    </div>\n  );\n}\n\nexport default App;\n","/Users/connormckenna/General_Assembly/sei/phoenix/unit_4/Tales-from-The-Roasted-Lizard/client/src/services/auth.js",[],"/Users/connormckenna/General_Assembly/sei/phoenix/unit_4/Tales-from-The-Roasted-Lizard/client/src/layouts/Layout.jsx",[],"/Users/connormckenna/General_Assembly/sei/phoenix/unit_4/Tales-from-The-Roasted-Lizard/client/src/components/Header.jsx",[],"/Users/connormckenna/General_Assembly/sei/phoenix/unit_4/Tales-from-The-Roasted-Lizard/client/src/services/apiConfig.js",[],"/Users/connormckenna/General_Assembly/sei/phoenix/unit_4/Tales-from-The-Roasted-Lizard/client/src/screens/Intro/Intro.jsx",[],"/Users/connormckenna/General_Assembly/sei/phoenix/unit_4/Tales-from-The-Roasted-Lizard/client/src/screens/Login/Login.jsx",[],"/Users/connormc